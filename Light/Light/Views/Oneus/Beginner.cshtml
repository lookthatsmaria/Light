@{ ViewData["Title"] = "To Moon-Starter Pack";
}
<!DOCTYPE html>
<html>
<head>
    <style>
        iframe {
            display: block;
            margin-left: auto;
            margin-right: auto;
        }
        img {
            padding: 10px;
        }
        .sidenav {
            height: 100%;
            width: 0;
            position: fixed;
            z-index: 1;
            top: 0;
            left: 0;
            background-color: #111;
            overflow-x: hidden;
            transition: 0.5s;
            padding-top: 60px;
        }

            .sidenav a {
                padding: 8px 8px 8px 32px;
                text-decoration: none;
                font-size: 25px;
                color: indianred;
                display: block;
                transition: 0.3s;
            }

                .sidenav a:hover {
                    color: #f1f1f1;
                }

            .sidenav .closebtn {
                position: absolute;
                top: 0;
                right: 25px;
                font-size: 36px;
                margin-left: 50px;
            }

        #main {
            transition: margin-left .5s;
            padding: 16px;
        }
    </style>
</head>
<body>
    <div id="mySidenav" class="sidenav">
        <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
        <a asp-controller="Oneus" asp-action="Beginner">Starter Pack</a>
        <a asp-controller="Oneus" asp-action="Fancafe">Fancafé</a>
    </div>
    <div id="main">
        <span style="font-size:30px;cursor:pointer" onclick="openNav()">&#9776;</span>

        <div class="text-center">

            <h1> Guides To Stan Oneus</h1>
            <p>[DISCLAIMER] None of this videos are mine. Credits to the owner! I just chose the most helpful guides from my point of view.</p>
            <iframe width="966" height="543" src="https://www.youtube.com/embed/Vu4uOODH_gg" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
            <p>[Suki Kirai] A Detailed Introduction to ONEUS 2021</p>
            <iframe width="966" height="543" src="https://www.youtube.com/embed/O2frrKm5x_I" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
            <p>[dazzlitez] a (most likely) helpful guide to oneus</p>
        </div>
        <iframe src="https://docs.google.com/forms/d/e/1FAIpQLSfYM3zFXMwSx0L09q2Q1WCY-os2G1BNxKfaMTt_2rQYGMDokA/viewform?embedded=true" embedded=true"
                width="100%"
                height="3000px"
                frameborder="0"
                marginheight="0"
                marginwidth="0">
            Loading…
        </iframe>

        <h2>Links to Oneus' Official Social Media Accounts</h2>
        <a href="https://www.youtube.com/channel/UCWTYf4xModfNsyq2NUYjF5g">
            <img src="https://assets.stickpng.com/images/580b57fcd9996e24bc43c545.png" alt="Link to Oneus' Official Youtube Channel" style="width:10%">
        </a>
        <a href="https://twitter.com/official_ONEUS">
            <img src="https://cdn4.iconfinder.com/data/icons/various-icons-2/476/Twitter.png" alt="Link to Oneus Official Twitter" style="width:8%">
        </a>
        <a href="https://www.instagram.com/official_oneus/">
            <img src="https://shangay.com/wp-content/uploads/2018/06/instagram-lgtb.jpg" alt="Link to Oneus Official Instagram" style="width:20%">
        </a>
        <a href="https://cafe.daum.net/RBWBOYZ">
            <img src="data:image/png;base64,"
                 alt="Link to Oneus Official Daum Cafe" style="width:20%">
        </a>
        <a href="https://www.vlive.tv/channel/C2D78B">
            <img src="https://upload.wikimedia.org/wikipedia/commons/a/a8/VLive_LOGO.png" alt="Link to Oneus Official Vlive account" style="width:15%">
        </a>
        <a href="https://www.facebook.com/officialONEUS">
            <img src="https://st4.depositphotos.com/5225467/23021/v/600/depositphotos_230217770-stock-illustration-facebook-logo-vector-editorial.jpg"
                 alt="Link to Oneus' Official Facebook account" style="width:13%">
        </a>
    </div>
    <script>function openNav() {
            document.getElementById("mySidenav").style.width = "250px";
            document.getElementById("main").style.marginLeft = "250px";
        }

        function closeNav() {
            document.getElementById("mySidenav").style.width = "0";
            document.getElementById("main").style.marginLeft = "0";
        }</script>
</body>
</html>

